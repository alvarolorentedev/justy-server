{
  "name": "justy-server",
  "version": "0.0.1",
  "description": "server side for justy app",
  "main": "src/server.ts",
  "scripts": {
    "test": "jest",
    "test:dev": "jest --watchAll",
    "test:cov": "jest --coverage && cat ./coverage/lcov.info",
    "start:local": "NODE_ENV=development ts-node .",
    "start": "NODE_ENV=production ts-node .",
    "start:dev": "NODE_ENV=development ts-node-dev .",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "migrate:local:init": "touch local.sqlite && NODE_ENV=development db-migrate up",
    "migrate:local:up": "db-migrate up -e development",
    "migrate:remote:up": "db-migrate up -e production",
    "migrate:local:down": "db-migrate down -e development",
    "migrate:remote:down": "db-migrate down -e production",
    "migrate:create": "db-migrate create"
  },
  "repository": "git@github.com:kanekotic/justy-server.git",
  "author": "kanekotic <alvarojosepl@gmail.com>",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "apollo-server-express": "2.14.1",
    "bcrypt": "4.0.1",
    "cors": "2.8.5",
    "datasource-sql": "1.3.0",
    "express": "4.17.1",
    "express-basic-auth": "1.2.0",
    "graphql": "15.0.0",
    "graphql-import": "1.0.2",
    "morgan": "1.10.0",
    "pg": "8.2.1",
    "ts-node": "8.10.2",
    "typescript": "3.9.3",
    "uuid": "8.1.0",
    "winston": "3.2.1",
    "winston-loggly-bulk": "3.1.0"
  },
  "devDependencies": {
    "@types/cors": "2.8.6",
    "@types/express": "4.17.6",
    "@types/graphql": "14.5.0",
    "@types/jest": "25.2.3",
    "@types/node": "14.0.6",
    "@types/uuid": "8.0.0",
    "@typescript-eslint/eslint-plugin": "3.1.0",
    "@typescript-eslint/parser": "3.0.1",
    "db-migrate": "0.11.11",
    "db-migrate-pg": "1.2.2",
    "db-migrate-sqlite3": "0.4.0",
    "eslint": "7.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-prettier": "3.1.3",
    "faker": "4.1.0",
    "jest": "26.0.1",
    "nodemon": "2.0.4",
    "prettier": "2.0.5",
    "sqlite3": "4.2.0",
    "ts-jest": "26.1.0",
    "ts-node-dev": "1.0.0-pre.44"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  }
}
